{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "DragonSweeper",
  "version": "0.1.0",
  "identifier": "com.dragonsweeper.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": null,
    "beforeDevCommand": null,
    "beforeBuildCommand": null
  },
  "app": {
    "windows": [
      {
        "title": "DragonSweeper",
        "width": 780,
        "height": 680,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
